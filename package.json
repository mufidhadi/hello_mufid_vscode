{
  "name": "hellomufid",
  "displayName": "hellomufid",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "./resource/logo.png",
  "main": "./extension.js",
  "repository": {
    "url": "https://github.com/mufidhadi/hello_mufid_vscode"
  },
  "contributes": {
    "commands": [
      {
        "command": "hellomufid.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "hellomufid.revealMupidChat",
        "title": "Reveal mupid jokes view"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mupid-chat-container",
          "title": "mupid jokes",
          "icon": "resource/icon.svg"
        }
      ]
    },
    "views": {
      "mupid-chat-container": [
        {
          "id": "mupidChatView",
          "name": "mupid jokes",
          "icon": "resource/icon.svg",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.39.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
